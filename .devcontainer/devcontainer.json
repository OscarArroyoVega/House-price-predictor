{
    "name": "Rust Devcontainer",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "workspaceFolder": "/workspace",
    "settings": {},
    "extensions": [
        "rust-lang.rust-analyzer"
    ],
    "remoteUser": "vscode",
    "postCreateCommand": "cargo build",
    "containerEnv": {
        "RUST_BACKTRACE": "1"
    }
}

